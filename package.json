{
  "name": "@duplojs/utils",
  "version": "0.0.0",
  "author": "mathcovax",
  "license": "MIT",
  "type": "module",
  "repository": "https://github.com/duplojs/utils",
  "scripts": {
    "build": "rollup --config && tsc-alias -p tsconfig.build.json",
    "test:tu": "vitest --coverage",
    "test:tu:watch": "vitest --coverage --watch",
    "test:tu:update": "vitest --coverage --update",
    "test:types": "tsc -p tsconfig.test.json",
    "test:lint": "eslint",
    "test:lint:fix": "eslint --fix",
    "prepare": "husky"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@duplojs/eslint": "0.5.0",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/node": "24.3.0",
    "@vitest/coverage-istanbul": "3.2.4",
    "eslint": "9.34.0",
    "husky": "9.1.7",
    "rollup": "4.50.0",
    "rollup-plugin-esbuild": "6.2.1",
    "tsc-alias": "1.8.16",
    "tslib": "2.8.1",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "keywords": [],
  "engines": {
   "node": ">=22.14.0"
  }
}
